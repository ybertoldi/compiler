<STMTS> ::= <STMTS> <STMT> | <STMT>;
<STMT>  ::= <DEC> 
          | <EXPR> TKTYPE_SMCOLON
          | <ASSIGN>
          | <IF>
          | <IF_ELSE>
          | <WHILE>;


<WHILE> ::= TKTYPE_WHILE TKTYPE_OPPAREN <EXPR> TKTYPE_CLPAREN
            TKTYPE_OPBRACKET <STMTS> TKTYPE_CLBRACKET;

<IF> ::= TKTYPE_IF TKTYPE_OPPAREN <EXPR> TKTYPE_CLPAREN TKTYPE_OPBRACKET <STMTS> TKTYPE_CLBRACKET;
<IF_ELSE> ::=  <IF> <ELSE>;
<ELSE> ::= TKTYPE_ELSE TKTYPE_OPBRACKET <STMTS> TKTYPE_CLBRACKET
         | TKTYPE_ELSE <IF>;

<DEC>   ::= <TIPO> TKTYPE_VAR TKTYPE_ATTRIBUTION <EXPR> TKTYPE_SMCOLON;
<ASSIGN> ::= TKTYPE_VAR TKTYPE_ATTRIBUTION <EXPR> TKTYPE_SMCOLON;

<EXPR>  ::= <LOG_OR>
          | <EXPR1>;
<LOG_OR> ::= <EXPR> TKTYPE_LOGIC_OR <EXPR1>;

<EXPR1> ::= <LOG_AND>
          | <EXPR2>;
<LOG_AND> ::= <EXPR1> TKTYPE_LOGIC_AND <EXPR2>;

<EXPR2> ::= <CMP_EQ>
          | <CMP_NEQ>
          | <EXPR3>;
<CMP_EQ>  ::= <EXPR2> TKTYPE_EQUALS <EXPR3>;
<CMP_NEQ> ::= <EXPR2> TKTYPE_NEQUALS  <EXPR3>;

<EXPR3>  ::= <CMP_LT>
           | <CMP_LEQT>
           | <CMP_GT>
           | <CMP_GEQT>
           | <EXPR4>;
<CMP_LT>   ::= <EXPR3> TKTYPE_LT <EXPR4>;
<CMP_LEQT> ::= <EXPR3> TKTYPE_LEQT  <EXPR4>;
<CMP_GT>   ::= <EXPR3> TKTYPE_GT    <EXPR4>;
<CMP_GEQT> ::= <EXPR3> TKTYPE_GEQT  <EXPR4>;

<EXPR4> ::= <SUM>
          | <SUB>
          | <EXPR5>;
<SUM> ::= <EXPR4> TKTYPE_SUM  <EXPR5>;
<SUB> ::= <EXPR4> TKTYPE_SUB  <EXPR5>;

<EXPR5> ::= <MULT>
          | <DIV>
          | <EXPR6>;
<MULT> ::= <EXPR5> TKTYPE_MULT  <EXPR6>;
<DIV>  ::=  <EXPR5> TKTYPE_DIV   <EXPR6>;


<EXPR6> ::= TKTYPE_VAR  
          | <CONST> 
          | <PAREN_EXPR>
          | <FUNC_CALL>;

<PAREN_EXPR> ::= TKTYPE_OPPAREN <EXPR> TKTYPE_CLPAREN;
<FUNC_CALL> ::= TKTYPE_VAR TKTYPE_OPPAREN <ARGS> TKTYPE_CLPAREN;
<ARGS> ::= <ARGS> TKTYPE_COMMA <EXPR> | <EXPR>;


<CONST> ::= TKTYPE_INT 
          | TKTYPE_FLOAT 
          | TKTYPE_HEXINT 
          | TKTYPE_OCTALINT 
          | TKTYPE_FALSE 
          | TKTYPE_TRUE 
          | TKTYPE_STRLIT;

<TIPO>  ::= TKTYPE_T_INT 
          | TKTYPE_T_CHAR 
          | TKTYPE_T_BOOL 
          | TKTYPE_T_LONG 
          | TKTYPE_T_FLOAT 
          | TKTYPE_T_DOUBLE;
